BAPP=4
ID=Gpredict
Name=Gpredict
Comment='real-time satellite tracking and orbit prediction application'
VerLocal=0
VerRemote=0
W3='http://gpredict.oz9aec.net/index.php'
Author='spud'
NOTE=''

INSTALL(){
    cd ${BAPSRC} || return

    sudo rm -rf gpredict
    git clone https://github.com/csete/gpredict
    cd gpredict
    ./configure
    make
    sudo make install 
}

VERSION(){
    if [[ $(whereis gpredict | grep bin) ]];then
        CURRENT='0-git'
    else
        CURRENT=NONE
    fi
    NEWVER='0-git'
}

DEPENDS(){
    NEEDED=''
    #apt-gets will be checked on app-check
}
