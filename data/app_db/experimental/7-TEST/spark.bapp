BAPP=4
ID=SPARK
Name=SPARK
Comment='SPARK, a Software Defined Radio (SDR)'
VerLocal=0
VerRemote=0
W3='https://www.sparksdr.com/manual'
Author='spud'
NOTE='Hermes-Lite and other SDRs'

INSTALL(){
	echo -e "INFORMATIONAL: apt-get required packages"
    
    cd ${BAPSRC} || return
    wget https://www.sparksdr.com/download/SparkSDR.2.0.33.linux-arm64.deb
    deb -i SparkSDR.2.0.33.linux-arm64.deb

}

VERSION(){
    if [[ $(whereis SparkSDR | grep bin) ]];then
        CURRENT=0-git
    else
        CURRENT=NONE
    fi
    NEWVER=0-deb
}

DEPENDS(){
    NEEDED=''
    #apt-gets will be checked on app-check
}
